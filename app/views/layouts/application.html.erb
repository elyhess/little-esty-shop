<!DOCTYPE html>
<html>
  <head>
    <title>LittleEtsyShop</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
    <section>
      <p>Repo Name:
        <%= @repo.repo_name %>
      Commits Count:
    <%= @repo.commits %>
      All Pull Requests Count:
    <%= @repo.pulls %>
      Collaborators:
    <%= @repo.collabs %>
    </section

  </body>
</html>
